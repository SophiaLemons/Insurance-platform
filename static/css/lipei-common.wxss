.progress-content{
	padding-top: 40rpx;
	padding-bottom: 30rpx;
	background-color: #fff;
	margin-bottom: 20rpx;
}
.progress-image{
	display: flex;
	justify-content: center;
}
.lipei-image{
	width: 550rpx;
	height: 80rpx;
}
.progress-text{
	padding: 10rpx 22rpx 0;
	display: flex;
	flex-flow: row nowrap;
	justify-content: space-around;
	font-size: 24rpx;
	color: #9E9E9E;
}
.progress-text-active{
	color: #FFA600;
}
