
.product-banner{
	height: 336rpx;
	width: 100%;
	margin-top: 14rpx;
}
.banner{
	height: 100%;
	width: 100%;
}
.product-base-info{
	padding-bottom: 30rpx;
	border-bottom: 2rpx solid #ECECEC;
	position: relative;
	margin-bottom: 9rpx;
}
.product-name{
	color: #282828;
	font-size: 40rpx;
	height: 88rpx;
	line-height: 88rpx;
}
.strongPoint{
	display: flex;
	flex-flow: row nowrap;
	margin-bottom: 42rpx;
}
.strongPoint view{
	display: flex;
	padding: 0 20rpx;
	height: 34rpx;
	line-height: 34rpx;
	font-size: 28rpx;
	color: #9E9E9E;
	position: relative;
}
.strongPoint view:first-child{
	padding-left: 0;
}
.strongPoint view:last-child::after{
	width: 0;
}
.strongPoint view::after{
	content: "";
	position: absolute;
	top: 7rpx;
	height: 20rpx;
	width: 2rpx;
	right: 0;
	background-color: #ECECEC;
}
.product-tag{
	display: flex;
	flex-flow: row nowrap;
}
.product-tag view{
	display: flex;
	height: 30rpx;
	line-height: 30rpx;
	font-size: 24rpx;
	margin-right: 40rpx;
}
.product-tag view:last-child{
	margin-right: 0;
}
.tag-label{
	color: #9E9E9E;
}
.tag-value{
	color: #282828;
	margin-left: 10rpx;
}
.insurance-logo{
	position: absolute;
	top: 72rpx;
	right: 0;
	height: 60rpx;
	width: 108rpx;
}
.product-plans{
	border-bottom: 2rpx solid #ececec;
}
.plans-top{
	padding: 30rpx 0;
	display: flex;
	flex-flow: row nowrap;
	font-size:32rpx;
	color: #282828;
	justify-content: space-between;
}
.plans-title{
	padding:0 4rpx;
	display: flex;
	line-height: 40rpx;
	margin-left: -4rpx;
}
.plans-top .title-active{
	background-size:100% 16rpx;
	background-position:bottom 0rpx left;
}
.bfss-btn{
	display: flex;
	width: 164rpx;
	height: 42rpx;
	background-image: url(http://file.baozhixing.com/culcater.png);
	background-repeat: no-repeat;
	background-position: left center;
	background-size: 42rpx 42rpx;
}
.bfss-btn{
	line-height: 42rpx;
	font-size: 28rpx;
	display: flex;
	justify-content: flex-end;
}
.plan-items{
	height: 70rpx;
	width: 100%;
	border-radius:10rpx;
	display: flex;
	flex-flow: row nowrap;
	margin-bottom: 30rpx;
  font-size: 28rpx;
  color: #9E9E9E;
}
.plan-item{
	flex:1;
	display: flex;
	justify-content: center;
	align-items: center;
	border:2rpx solid rgba(236,236,236,1);
	border-right: none;
}
.plan-item:last-child{
	border-right: none;
	border-top-right-radius: 10rpx;
	border-bottom-right-radius: 10rpx;
	border-right:2rpx solid rgba(236,236,236,1);
}
.plan-item:first-child{
	border-top-left-radius: 10rpx;
	border-bottom-left-radius: 10rpx;
}
.plan-item-active{
	color: #fff;
	background-color: #ffa600;
	border-right: none;
	position: relative;
	border:2rpx solid #ffa600;
}
.plan-item-active::after{
	position: absolute;
	content: "";
	height: 17rpx;
	width: 17rpx;
	bottom: -8rpx;
	left: 50%;
	margin-left: -9rpx;
	background-color: #ffa600;
	transform: rotate(45deg);
}
.suportShame-item{
	height: 40rpx;
	line-height: 40rpx;
	font-size: 32rpx;
	color: #636363;
	margin-bottom: 20rpx;
}
.item-left{
	float: left;
	width: 65%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.item-right{
	float: right;
	width: 35%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	text-align: right;
}
.look-suport-detail{
	padding: 10rpx 0 20rpx 0;
	font-size: 28rpx;
	color: #185DD0;
}
.baofei{
	display: flex;
	padding: 20rpx 0 40rpx 0;
	flex-flow: row nowrap;
	justify-content: space-between;
}
.baofei-label{
	font-size: 28rpx;
	color: #9E9E9E;
	margin-top: 10rpx;
	/*display: flex;*/
}
.baofei-label-price{
	font-size: 28rpx;
	color: #9E9E9E;
}
.renminbi-icon{
	font-size: 24rpx;
	color: #FFA600;
}
.baofei-price{
