/*保费试算*/

.product-compute-dialog{
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background:rgba(0,0,0,0.22);
	z-index: 500;
}
.compute-footer{
	position: fixed;
	bottom: 0;
	left: 0;
	background-color: #fff;
	height: 100rpx;
	width: 100%;
	display: flex;
	flex-flow: row nowrap;
	z-index: 1000;
}
.product-compute{
	position: fixed;
	left: 0;
	right: 0;
	top:300rpx;
	bottom: 0;
	background:#fff;
	z-index: 1000;
	border-radius:20rpx 20rpx 0px 0px;
}

.product-compute{
	padding-left: 40rpx;
}
.compute-title-container{
	padding-right: 40rpx;
}
.compute-title{
	height: 114rpx;
	line-height: 114rpx;
	font-size: 36rpx;
	color: #282828;
	position: relative;
}
.compute-close{
	height: 100%;
	width: 103rpx;
	position: absolute;
	right: -40rpx;
	top: 0;
	background-image: url(http://file.baozhixing.com/cancle@2x.png);
	background-repeat: no-repeat;
	background-size: 24rpx 24rpx;
	background-position:center center;
}
.compute-items{
	width: 100%;
	overflow-y: scroll;
	-webkit-overflow-scrolling:touch;
	padding-top: 30rpx;
	position: absolute;
	top: 114rpx;
	bottom: 100rpx;
}
.compute-item{
	height: 68rpx;
	width: 100%;
	display: flex;
	flex-flow: row nowrap;
	font-size:28rpx;
	color: #636363;
	margin-bottom: 50rpx;
}
.compute-item-label{
	width: 140rpx;
	height: 68rpx; 
	display: flex;
	align-items: center;
}
.compute-item-items{
	white-space: nowrap;
	overflow-x: scroll;
	width: 550rpx;
	margin-left: 20rpx;
	padding-top: 6rpx;
}
.compute-item-item{
	height: 56rpx;
	width: 132rpx;
	text-align: center;
	display: inline-block;
	border:2rpx solid rgba(236,236,236,1);
	margin-right: 20rpx;
	border-radius: 32rpx;
	line-height: 56rpx;
}
.lengthMore3{
	height: 56rpx;
	padding: 0 24rpx;
	width: initial;
}
.compute-item-active{
	background-color: #ffa600;
	border:2rpx solid #ffa600;
	color: #fff;
}
